{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/jobNew.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1
    },
    "salary": {
      "type": "integer",
      "minLength": 1,
      "maxLength": 10
    },
    "equity": {
      "type": "number"
    },
    "companyHandle": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": ["title", "salary", "equity", "companyHandle"]
}
